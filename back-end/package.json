{
  "name": "alignment-backend",
  "version": "1.0.0",
  "description": "Backend for alignment project using Zod schemas",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev src/server.ts",
    "build": "tsc",
    "test": "vitest run",
    "test:terse": "vitest run --reporter=dot",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "convert-sample-json-to-csv": "ts-node scripts/json-to-csv.ts samples/sample_open_questions.json samples/sample_open_questions.csv",
    "convert-errorful-json-to-csv": "ts-node scripts/json-to-csv.ts samples/errorful_open_questions.json samples/errorful_open_questions.csv"
  },
  "dependencies": {
    "@types/multer": "^1.4.12",
    "@zodyac/zod-mongoose": "^3.2.0",
    "bcrypt": "^5.1.1",
    "csv-parse": "^5.6.0",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "supertest": "^7.1.0",
    "zod": "*"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.4.2",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-v8": "^3.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "^3.1.1"
  }
}
